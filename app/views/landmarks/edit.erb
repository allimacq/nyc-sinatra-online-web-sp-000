<h2>Edit Historical Landmark</h2>
<form method="post" action="/landmarks/<%=@landmark.id %>">
  <input id="hidden" type="hidden" name="_method" value="patch">

  <label>Edit Landmark Name</label>
  <input type="text" name="landmark[name]" id="new_landmark" value="landmark[name]"><br>
  
   <label>Edit Year Completed</label>
  <input type="text" name="landmark[year_completed]" id="new_landmark" value="<%= @landmark.year_completed.to_s %>"><br>
  
  <input type="submit" value="Edit Landmark">
</form>
<a class="button" href="/landmarks">Landmarks</a>
<a class="button" href="/">Home</a>
